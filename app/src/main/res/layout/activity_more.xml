<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_more"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.wenzhiguo.yunifang.activity.MoreActivity">
    <com.wenzhiguo.yunifang.Custom.TitleBar
        android:id="@+id/more_title"
        app:img_imageView="@drawable/back_pressed"
        app:mText="更多商品"
        app:mTextSize="30sp"
        android:layout_width="match_parent"
        android:layout_height="50dp"/>
    <View
        android:id="@+id/shopping_view"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#778899" />
    <GridView
        android:numColumns="2"
        android:id="@+id/more_gridview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></GridView>
</LinearLayout>
